{
  "name": "template-frontend-spa-react",
  "version": "1.0.0",
  "description": "Шаблон проекта фронтенд",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexeyAbretov/ProjectTemplateFrontendSpaReact.git"
  },
  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" webpack --config=\"webpack/webpack.prod-config.ts\" --stats-error-details",
    "build:dev": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" BUILD_MODE=\"build-dev\" webpack --config=\"webpack/webpack.dev-config.ts\" --stats-error-details",
    "start": "cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\" webpack serve --config=\"webpack/webpack.dev-config.ts\"",
    "test": "jest",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test:clear": "jest --clearCache",
    "test:changes": "jest --onlyChanged",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:onFirstFail": "jest --bail",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
