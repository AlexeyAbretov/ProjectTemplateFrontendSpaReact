    "@storybook/react-webpack5": "10.2.5",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/jest": "30.0.0",
    "@types/node": "25.2.0",
    "@types/react": "19.2.11",
    "@types/react-dom": "19.2.3",
